package day0;

public class StringReverse {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String str ="kavitha";
		String rever=" ";
		char ch;
		for(int i=0;i<str.length();i++)
		{
		ch=str.charAt(i);
		rever = ch+rever;
		
		}
		
		System.out.println("Reverse string:    "  + rever);

	}

}
